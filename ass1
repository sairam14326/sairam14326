#general method
n=int(input('enter the year:'))
if (n%400)==0 or (n%4)==0 and (n%100)!=0:
    print('the given year is leap year')
else:
    print('the given year is not leap year')
    
    
    
    #using function    
def leapyear(n):
    return (n%400)==0 or (n%4)==0 and (n%100)!=0 
n=int(input())
if(leapyear(n)):      #calling function
    print("true")
else:
    print("false")
    
